#!/usr/bin/env python
# coding: utf-8

import tkinter as tk

class ClassName(tk.Frame):
    """Documentation for ClassName

    """
    def __init__(self, master):
        super().__init__(master)

        self.title("TODO APP")
        
        self.pack()
        


root = tk.Tk()
root.title("TODO APP")
todoapp = ClassName(root)
todoapp.mainloop()
